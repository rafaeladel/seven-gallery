<div>
  <%= f.label :caption %>
  <%= f.text_field :caption %>
</div>
<div>
  <%= f.label :description %>
  <%= f.text_area :description %>
</div>
<div>
  <%= f.label :is_featured %>
  <%= f.check_box :is_featured %>
</div>
<div>
  <%= f.label :alt %>
  <%= f.text_field :alt %>
</div>
<% %w[x y w h].each do |attribute| %>
    <%= f.hidden_field "crop_#{attribute}", class: "crop_#{attribute}" %>
<% end %>